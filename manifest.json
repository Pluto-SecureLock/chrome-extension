{
  "manifest_version": 3,
  "name": "Pluto Extension",
  "version": "0.1",
  "description": "Pluto Extension Helper Extension",
  "author": "Giorgi Japaridze",
  "background": {
  "service_worker": "background.js"
},
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "sources/Icon_background.png",
      "32": "sources/Icon_background.png",
      "64": "sources/Icon_background.png",
      "128": "sources/Icon_background.png"
    },
    "default_title": "Pluto Helper"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]

    }],

"icons": {
    "16": "sources/Icon_background.png",
    "32": "sources/Icon_background.png",
    "64": "sources/Icon_background.png",
    "128": "sources/Icon_background.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "windows",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
